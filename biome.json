{
	"$schema": "https://biomejs.dev/schemas/1.5.3/schema.json",
	"organizeImports": { "enabled": true },
	"formatter": {
		"enabled": true,
		"formatWithErrors": false,
		"indentStyle": "tab",
		"indentWidth": 2,
		"lineWidth": 160,
		"lineEnding": "lf",
		"ignore": []
	},
	"javascript": {
		"formatter": {
			"quoteStyle": "single",
			"semicolons": "asNeeded"
		}
	}
}
